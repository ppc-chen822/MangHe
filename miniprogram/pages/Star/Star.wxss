.container{
  position: relative;
}
.container .box{
  width: 100%;
  margin-top: 80rpx;
}
.container .box .list-one{
  width: 90%;
  margin: 0 auto;
  margin-top: 40rpx;
  display: flex;
}
.container .box .list-one>view:first-child{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  border: 1px solid rgba(0,0,0,.2);
  overflow: hidden;
}
.container .box .list-one>view:first-child>image{
  width: 100%;
  height: 100%;
}
.container .box .list-one>view:nth-child(2){
  width: 80%;
}
.container .box .list-one .Sname{
  overflow: hidden;
}
.container .box .list-one .Sname text{
  font-size: 36rpx;
  font-family: '微软雅黑';
  margin-left: 20rpx;
  float: left;
  font-weight: 550;
}
.container .box .list-one .Sname>view>image{
  width: 36rpx;
  height: 36rpx;
  margin-left: 20rpx;
  margin-top: 8rpx;
}
.container .box .list-one .detail-info{
  width: 100%;
  margin-top: 10rpx;
}
.container .box .list-one .detail-info span{
  font-size: 24rpx;
  font-family: '微软雅黑';
  margin-left: 20rpx;
}
.container .content .list-two{
  width: 90%;
  margin: 0 auto;
}
.container .content .list-two .two-text{
  font-size: 32rpx;
   /* 定义为自适应布局   */
   display: -webkit-box;       
   /* 超出部分隐藏 */
   overflow: hidden;
   /* 竖直方向的超出隐藏 */
   -webkit-box-orient: vertical;  
   /* 限制最多2行 */
   -webkit-line-clamp: 3;
}
.container .content .list-two .two-img{
  display: flex;
}
.container .content .list-two .two-img>view{
  flex: 1;
  flex-direction: column;
  width: 50%;
  height: 350rpx;
  margin-top: 40rpx;
}
.container .content .list-two .two-img>view image{
  width: 95%;
  height: 95%;
  border-radius: 20rpx;
}
.container .content .list-two .two-img>view:nth-child(2) image{
  float: right;
}
.container .content .list-three{
  width: 90%;
  margin: 0 auto;
  display: flex;
  justify-content: space-between;
}
.container .content .list-three .three-icon{
  float: left;
}
.container .content .list-three .three-icon>image{
  width: 40rpx;
  height: 40rpx;
  vertical-align: bottom;
  margin-right: 10rpx;
}
.container .content .list-three .three-icon>span{
  margin-right: 10rpx;
}
.container .Fabu{
  width: 150rpx;
  height: 150rpx;
  position: fixed;
  bottom: 40rpx;
  right: 20rpx;
  border-radius: 50%;
  background-color: #fff;
  padding: 30rpx;
  box-sizing: border-box;
}
.container .Fabu image{
  width: 100%;
  height: 100%;
}