.container,page {
  background: #fff;
}
.container .form .send-title{
  font-size: 36rpx;
  font-weight: bold;
  font-family: '微软雅黑';
  text-align: center;
}
.container .form{
  margin-top: 40rpx;
}
.container .form .f-radio{
  margin-top: 40rpx;
}
.i-female,.i-male {
  width: 50rpx;
  height: 50rpx;
  display: block;
}

.radio1 {
  margin-right: 30rpx;
}

.radio {
  margin-right: 10rpx;
}

.form {
  width: 90%;
  margin: 0 auto;
}

.form-group {
  border-bottom: 2rpx solid #eee;
  margin-top: 30rpx;
  border-radius: 16rpx;
}

.cu-input,.form-group {
  height: 100rpx;
  line-height: 100rpx;
}

.cu-input {
  padding-left: 20rpx;
  box-sizing: border-box;
  font-size: 30rpx;
  border-radius: 20rpx;
}

