button{
  width: 200rpx;
  display: block;
  height: 80rpx;
  line-height: 80rpx;
  margin: 60rpx auto;
  background-color: #a18cd1;
}


.tui-picker-content{
  margin-top: 30rpx;
  height: 100rpx;
  border-bottom: 2rpx solid #eee;
  border-radius: 16rpx;
  padding-top: 10rpx;
}
.tui-picker-name{
  height: 80rpx;
  line-height: 80rpx;
}
.tui-picker-detail{
  height: 80rpx;
  line-height: 80rpx;
  background-color: #fff;
  font-size: 35rpx;
  padding: 0 20rpx;
  overflow: hidden;
}


.image-list{
  width: 150rpx;
  height: 150rpx;
  margin-top: 20rpx;
  position: relative;
}
.image-wrap{
  position: absolute;
  top: 0;
}
.selectphoto{
  border: 4rpx dashed #888;
  border-radius: 6rpx;
}
.image{
  width: 100%;
  height: 100%;
}

